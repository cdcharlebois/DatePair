<?xml version="1.0" encoding="utf-8" ?>
<widget id="DatePair.widget.Date" needsEntityContext="true" 
  xmlns="http://www.mendix.com/widget/1.0/">
  <name>Date</name>
  <description>The description of this widget.</description>
  <icon></icon>
  <properties>
    <property key="fromDate" type="attribute" required="true" isPath="no">
      <caption>Datetime</caption>
      <category>General</category>
      <description></description>
      <attributeTypes>
        <attributeType name="DateTime" />
      </attributeTypes>
    </property>
    <property key="editable" type="boolean" required="true" defaultValue="true">
      <caption>Editable</caption>
      <category>General</category>
      <description></description>
    </property>
    <property key="displayAsModal" type="boolean" required="true" defaultValue="true">
      <caption>Display As Modal?</caption>
      <category>General</category>
      <description>Use the modal stylesheet</description>
    </property>
    <property key="label" type="string" required="false" defaultValue="">
      <caption>Label</caption>
      <category>General</category>
      <description></description>
    </property>
    <property key="labelWidth" type="integer" required="true" defaultValue="0">
      <caption>Label Width</caption>
      <category>General</category>
      <description>How many (of the 12) columns should the label take up? Leave as 0 to infer the width automagically (based on the previous sibling)</description>
    </property>
    <property key="showLabel" type="boolean" required="true" defaultValue="true">
      <caption>Show Label</caption>
      <category>General</category>
      <description></description>
    </property>
    <!-- DATE -->
    <property key="rangeMinNumber" type="string" required="true" defaultValue="0">
      <caption>Earliest Selectable Date</caption>
      <category>Date</category>
      <description>An integer (days, relative to today). i.e. -1 is yesterday, +2 is the day after tomorrow.</description>
    </property>
    <property key="rangeMaxNumber" type="string" required="true" defaultValue="0">
      <caption>Latest Selectable Date</caption>
      <category>Date</category>
      <description>An integer (days, relative to today). i.e. -1 is yesterday, +2 is the day after tomorrow.</description>
    </property>
    <property key="rangeMinDate" type="attribute" required="false" isPath="optional" pathType="reference">
      <caption>Earliest Selectable Date (attr)</caption>
      <category>Date</category>
      <description>A Datetime attribute representing the earliest selectable date. **Overrides the integer value above**</description>
      <attributeTypes>
        <attributeType name="DateTime" />
      </attributeTypes>
    </property>
    <property key="rangeMaxDate" type="attribute" required="false" isPath="optional" pathType="reference">
      <caption>Latest Selectable Date (attr)</caption>
      <category>Date</category>
      <description>A Datetime attribute representing the latest selectable date. **Overrides the integer value above**</description>
      <attributeTypes>
        <attributeType name="DateTime" />
      </attributeTypes>
    </property>
    <property key="dateFormat" type="string" required="true" defaultValue="m/d/yyyy">
      <caption>Date Format</caption>
      <category>Date</category>
      <description>See http://amsul.ca/pickadate.js/date/#formatting-rules for more info</description>
    </property>
    <property key="disabledDaysOfWeek" type="string" required="false" defaultValue="">
      <caption>Disabled Days</caption>
      <category>Date Restrictions</category>
      <description>Comma-separated list of days of the week. i.e. `1,4,7` = Sunday, Wednesday, and Saturday are disabled for picking.</description>
    </property>
    <property key="sourceentity" type="entity" required="false" allowNonPersistableEntities="true">
      <caption>Date Entity</caption>
      <category>Date Restrictions</category>
      <description>Entity with a datetime attribute that will be greyed out.</description>
    </property>
    <property key="disabledDatesMf" type="microflow" required="false">
      <caption>Microflow</caption>
      <category>Date Restrictions</category>
      <description>A Microflow that returns a list of an entity with a datetime attribute. Datetimes matching these values will be greyed out.</description>
      <returnType type="Object" isList="true" entityProperty="sourceentity"></returnType>
    </property>
    <property key="disabledDatesAttr" type="attribute" required="false" isPath="no" entityProperty="sourceentity">
      <caption>Date Attribute</caption>
      <category>Date Restrictions</category>
      <description>The Attribute which holds the date to be greyed out.</description>
      <attributeTypes>
        <attributeType name="DateTime" />
      </attributeTypes>
    </property>
    <property key="datePickerShouldStartOpen" type="boolean" required="true" defaultValue="false">
      <caption>Start Open?</caption>
      <category>Date</category>
      <description>Start the datepicker open</description>
    </property>
    <property key="datePlaceholder" type="string" required="false" defaultValue="Pick a date...">
      <caption>Date Placeholder</caption>
      <category>Date</category>
      <description>The placeholder for the date input</description>
    </property>
    <property key="onChangeMf" type="microflow" required="false">
      <caption>On change</caption>
      <category>General</category>
      <description></description>
      <returnType type="Void" />
    </property>
    <!-- TIME -->
    <!-- <property key="timeFormat" type="string" required="true" defaultValue="h:i A"><caption>Time Format</caption><category>Time</category><description>See http://amsul.ca/pickadate.js/time/#formatting-rules for more info</description></property><property key="timeInterval" type="integer" required="true" defaultValue="30"><caption>Interval (Mins)</caption><category>Time</category><description></description></property><property key="rangeMinTimeNumber" type="string" required="true" defaultValue="0"><caption>Earliest Selectable Time</caption><category>Time</category><description>An integer (intervals, relative to now). i.e. -1 is one interval ago, +2 is two intervals from now. You may also enter time in 24h format i.e.  `08:30` for 8:30am or `16:45` for 4:45pm</description></property><property key="rangeMaxTimeNumber" type="string" required="true" defaultValue="0"><caption>Latest Selectable Time</caption><category>Time</category><description>An integer (intervals, relative to now). i.e. -1 is one interval ago, +2 is two intervals from now. You may also enter time in 24h format i.e. `08:30` for 8:30am or `16:45` for 4:45pm</description></property><property key="disabledTimeString" type="string" required="false" defaultValue=""><caption>Disabled Times</caption><category>Time Restrictions</category><description>Comma-separated list of times. i.e. `8:30, 13:00` would result in 8:30am and 1:00pm being disabled.</description></property><property key="sourceentitytime" type="entity" required="false" allowNonPersistableEntities="true"><caption>Time Entity</caption><category>Time Restrictions</category><description>Entity with a datetime attribute that will be greyed out.</description></property><property key="disabledTimesMf" type="microflow" required="false"><caption>Times Microflow</caption><category>Time Restrictions</category><description>A Microflow that returns a list of an entity with a datetime attribute. times matching these values will be greyed out.</description><returnType type="Object" isList="true" entityProperty="sourceentitytime"></returnType></property><property key="disabledTimesAttr" type="attribute" required="false" isPath="no" entityProperty="sourceentitytime"><caption>Time Attribute</caption><category>Time Restrictions</category><description>The Attribute which holds the time to be greyed out.</description><attributeTypes><attributeType name="DateTime" /></attributeTypes></property> -->
  </properties>
</widget>
